{
  "name": "tln-cli",
  "version": "19.10.0",
  "description": "Multi-projects management tool",
  "main": "cli.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha **/*spec.js"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "bin": {
    "tln": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-talan/tln-cli.git"
  },
  "keywords": [
    "node",
    "cli",
    "management"
  ],
  "author": "Vladyslav Kurmaz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/project-talan/tln-cli/issues"
  },
  "homepage": "https://github.com/project-talan/tln-cli#readme",
  "dependencies": {
    "empty-dir": "^2.0.0",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "getos": "^3.1.1",
    "json-fn": "^1.1.1",
    "log4js": "^3.0.6",
    "tempfile": "^2.0.0",
    "yaml": "^1.6.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "mock-fs": "^4.10.1",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1"
  }
}
